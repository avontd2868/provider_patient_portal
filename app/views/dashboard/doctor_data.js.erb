$('#doctor_list').empty();
<% @docs.each do |doc| %>
  doc = $('<div>');
  doc.addClass('doctor_search');
  $('#doctor_list').append(doc)
  doc.append('<div> <%= doc.first_name %> <%= doc.last_name %> </div>');
  doc.append('<div> <%= doc.phone %> </div>');
  doc.append('<div> <%= doc.email %> </div>');
  doc.append('<div> <%= link_to "Send a Message", root_path %> </div>');
  doc.append('<div> <%= link_to "Set an Appointment", root_path %> </div>');
<% end %>

